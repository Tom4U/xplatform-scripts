{
    "cache": false,
    "check-coverage": true,
    "extension": [
        ".ts"
    ],
    "include": [
        "**/src/*.ts"
    ],
    "exclude": [
        "**/src/*.test.ts"
    ],
    "reporter": [
        "cobertura",
        "text-summary",
        "lcov"
    ],
    "statements": 80,
    "branches": 80,
    "functions": 80,
    "lines": 80
}